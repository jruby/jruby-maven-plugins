<project>
  <modelVersion>4.0.0</modelVersion>
  <groupId>com.example</groupId>
  <artifactId>typhoeus-test</artifactId>
  <version>0.0.0</version>
  <repositories>
    <repository>
      <id>target/rubygems-releases</id>
      <url>http://target/rubygems-proxy.torquebox.org/releases</url>
    </repository>
  </repositories>
  <dependencies>
    <dependency>
      <groupId>rubygems</groupId>
      <artifactId>typhoeus</artifactId>
      <version>0.2.4</version>
      <type>gem</type>
    </dependency>
  </dependencies>
  <properties>
    <root.dir>${basedir}/../../../../</root.dir>
    <gem.home>${root.dir}/target/rubygems</gem.home>
    <gem.path>${root.dir}/target/rubygems</gem.path>
  </properties>
  <build>
    <plugins>
      <plugin>
        <groupId>org.jruby.maven</groupId>
        <artifactId>gem-maven-plugin</artifactId>
        <version>@project.version@</version>
        <configuration>
          <supportNative>true</supportNative>
        </configuration>
      </plugin>
    </plugins>
  </build>
</project>
